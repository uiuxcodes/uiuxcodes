{
  "version": "0.0.8",
  "license": "MIT",
  "main": "dist/bootstrap/style.css",
  "files": [
    ".github/logo.png",
    "dist/bootstrap",
    "bootstrap"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "npm run prepare",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "npm run style",
    "css-compile": "node-sass --include-path node_modules --output-style compressed --source-map true --source-map-contents true --precision 6 bootstrap -o dist/bootstrap",
    "size": "size-limit",
    "style": "npm run css-compile && cssnano dist/bootstrap/style.css dist/bootstrap/style.min.css --no-zindex --no-reduceIdents",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "uiuxcodes",
  "author": "maxifjaved",
  "style": "dist/bootstrap/style.min.css",
  "sass": "bootstrap/style.scss",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "cssnano": "^5.1.7",
    "cssnano-cli": "^1.0.5",
    "husky": "^8.0.1",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.13",
    "postcss-cli": "^9.1.0",
    "sass": "^1.51.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@uiuxcodes/bootstrap": "^0.0.1"
  }
}
